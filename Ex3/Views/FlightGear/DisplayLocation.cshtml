@model Ex3.Models.DisplayLocation

@{
    ViewBag.Title = "DisplayLocation";
}

<img id="map" src="~/Image/map.png" alt="map" style="position:absolute; height:100%; width:100%"; />
<img  id="airplane" src="~/Image/airplane.png" alt="airplane" style="position:absolute; height:30px; width:30px"; />
<canvas id="airplaneCanvas" style="position:absolute; height:100%; width:100%" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<body>
    <script>
        var ctx = document.getElementById("airplaneCanvas").getContext("2d");
        document.getElementById("airplane").style.top = @this.Model.displayUtils.Lat + "px";
        document.getElementById("airplane").style.left = @this.Model.displayUtils.Lon + "px";
    </script>
</body>